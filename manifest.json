{
  "manifest_version": 2,

  "name": "Minion dummy text",
  "description": "A extension that will fill up forms with mionion text",
  "version": "1.0",

  "browser_action": {
      "default_icon": "icon.png"
  },

  "background": {
      "scripts": ["load.js"],
      "persistent": false
  },

  "permissions": [
      "activeTab"
   ]
}
